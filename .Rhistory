#print(borra)
mont<-mont[-borra,]
mont<-mont[-borra,]
if(np>6){
mont<-mont[-(borra+1),]
mont<-mont[-(borra+1),]
}
#        print(mont)
mont1<-div_pol_n(mont,recursivo)# añade caos
mont1<-rbind(mont1,c(mont1[nrow(mont1),1]+100,0))
polygon(mont1,col=gray(1-des*(i-1)), border=NA)
#print(mont1)
}
